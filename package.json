{
  "name": "hooprepublic",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "deploy": "npm run build && npx wrangler pages deploy .output/public",
    "db:create": "npx wrangler d1 create hooprepublic-db",
    "db:migrate": "npx wrangler d1 execute hooprepublic-db --file=./db/schema.sql",
    "db:migrate:local": "npx wrangler d1 execute hooprepublic-db --local --file=./db/schema.sql",
    "db:seed:generate": "npx tsx db/seed.ts > db/seed.sql",
    "db:seed": "npm run db:seed:generate && npx wrangler d1 execute hooprepublic-db --file=./db/seed.sql",
    "db:seed:local": "npm run db:seed:generate && npx wrangler d1 execute hooprepublic-db --local --file=./db/seed.sql"
  },
  "dependencies": {
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "dayjs": "^1.11.19",
    "leaflet": "^1.9.4",
    "nanoid": "^5.1.6",
    "nuxt": "^4.3.1",
    "stripe": "^20.3.1",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4",
    "vue3-google-signin": "^2.1.1"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.21",
    "@types/node": "^25.2.2",
    "tsx": "^4.21.0",
    "wrangler": "^4.63.0"
  }
}
